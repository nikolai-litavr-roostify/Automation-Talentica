<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" configfailurepolicy="continue">
    <listeners>
        <listener class-name="resources.extentReporterNG"/>
        <listener class-name="listener.TestListener"/>
    </listeners>
    <test verbose="2" preserve-order="true" name="AccountCheck" parallel="methods">
        <classes>
            <class name="accCheck.accountCheckTests"></class>
        </classes>
    </test>


    <test name="Finicity">
        <classes>
            <class name="finicity.finicityTests"></class>
        </classes>
    </test>

    <test name="Zillow">
        <classes>
            <class name="zillow.zillowTests"></class>
        </classes>
    </test>

    <test name="Plaid">
        <classes>
            <class name="plaid.plaidTests"></class>
        </classes>
    </test>

    <test name="PPE_HELOC" parallel="methods">
        <classes>
            <class name="ppe.ppe_OB_Tests"></class>
        </classes>
    </test>

    <test name="RcoreTest">
        <classes>
            <class name="rCore.rCoreTests"></class>
        </classes>
    </test>


</suite>
